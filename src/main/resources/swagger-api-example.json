            "required": false,
            "type": "string"
          },
          {
            "name": "file",
            "in": "formData",
            "description": "file to upload",
            "required": false,
            "type": "file"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/ModelApiResponse"
            }
          }
        },
        "security": [
          {
            "petclinic_auth": [
              "write:pets",
              "read:pets"
            ]
          }
        ]
