  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    },
    "petclinic_auth": {
      "type": "oauth2",
      "authorizationUrl": "https://petclinic.swagger.io/oauth/authorize",
      "flow": "implicit",
      "scopes": {
        "read:pets": "read your pets",
        "write:pets": "modify pets in your account"
      }
    }
  },
  "definitions": {
    "ModelApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "type": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "PetDto": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "user": {
          "$ref": "#/definitions/UserDto"
        },
        "name": {
          "type": "string",
          "example": "doggie"
        },
        "visits": {
          "type": "array",
          "xml": {
            "wrapped": true
          },
          "items": {
            "xml": {
              "name": "visit"
            },
            "$ref": "#/definitions/VisitDto"
          }
        }
      },
      "xml": {
        "name": "PetDto"
      }
    },
    "VisitDto": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "pet": {
          "$ref": "#/definitions/PetDto"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "veterinarianName": {
          "type": "string"
        },
        "clinicName": {
          "type": "string"
        }
      },
      "xml": {
        "name": "VisitDto"
      }
    },
    "UserDto": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "username": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "userStatus": {
          "type": "integer",
          "format": "int32",
          "description": "User Status"
        }
      },
      "xml": {
        "name": "UserDto"
      }
    }
  },
