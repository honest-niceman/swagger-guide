    "/clinic/visit/{visitId}": {
      "get": {
        "tags": [
          "clinic"
        ],
        "summary": "Find purchase visit by ID",
        "description": "For valid response try integer IDs with value >= 1 and <= 10. Other values will generated exceptions",
        "operationId": "getVisitById",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "name": "visitId",
            "in": "path",
            "description": "ID of pet that needs to be fetched",
            "required": true,
            "type": "integer",
            "maximum": 10,
            "minimum": 1,
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/VisitDto"
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Visit not found"
          }
        }
      },
      "delete": {
        "tags": [
          "clinic"
        ],
        "summary": "Delete purchase visit by ID",
        "description": "For valid response try integer IDs with positive integer value. Negative or non-integer values will generate API errors",
        "operationId": "deleteVisit",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "name": "visitId",
            "in": "path",
            "description": "ID of the visit that needs to be deleted",
            "required": true,
            "type": "integer",
            "minimum": 1,
            "format": "int64"
          }
        ],
        "responses": {
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "VisitDto not found"
          }
        }
      }
    },
    "/clinic/inventory": {
      "get": {
        "tags": [
